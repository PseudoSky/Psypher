{
"name":"Inject DOM",
"description":"http://stackoverflow.com/questions/14068879",
"version":"1",
"manifest_version":2,
"permissions": [
  "activeTab",
  "https://ajax.googleapis.com/",
  "tabs",
  "webRequest",
  "ftp://*/*",
  "http://*/*",
  "https://*/*",
  "background",
  "cookies",
  "debugger",
  "notifications",
  "storage",
  "webRequest",
  "webRequestBlocking"
],
"content_scripts": [
  {
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "css": ["lib/css/scrubber.css"],
    "js": ["lib/dexie.js","lib/sjcl.js","lib/collections.min.js","lib/mousetrap.js",
    "src/bender/index.js","src/bender/upgrades.js","lib/lodash.js","lib/d3.js","lib/jquery.min.js", "lib/scrubber.js",
    "lib/cryptico.js","src/psypher.js","myscript.js"],
    "run_at": "document_idle",
    "all_frames": true
  }
]
}
